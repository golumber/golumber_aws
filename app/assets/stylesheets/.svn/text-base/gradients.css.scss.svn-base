/*for all generic buttons*/	
.submitbutton {
	background: #0f811f;
	color: #eef6ed;
}
.submitbutton:hover {
	background: #0a7219;
	color: #eef6ed;
}

/*header and footer colors*/
.gradient1 {
	-moz-border-radius:6px;
	-webkit-border-radius:6px;
	border-radius:0px;
	background: rgb(1,136,181); /* Old browsers */
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(top, rgba(1,136,181,1) 0%, rgba(1,136,181,1) 48%, rgba(0,148,196,1) 79%, rgba(0,148,196,1) 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(1,136,181,1)), color-stop(48%,rgba(1,136,181,1)), color-stop(79%,rgba(0,148,196,1)), color-stop(100%,rgba(0,148,196,1))); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top, rgba(1,136,181,1) 0%,rgba(1,136,181,1) 48%,rgba(0,148,196,1) 79%,rgba(0,148,196,1) 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top, rgba(1,136,181,1) 0%,rgba(1,136,181,1) 48%,rgba(0,148,196,1) 79%,rgba(0,148,196,1) 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top, rgba(1,136,181,1) 0%,rgba(1,136,181,1) 48%,rgba(0,148,196,1) 79%,rgba(0,148,196,1) 100%); /* IE10+ */
background: linear-gradient(to bottom, rgba(1,136,181,1) 0%,rgba(1,136,181,1) 48%,rgba(0,148,196,1) 79%,rgba(0,148,196,1) 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#0188b5', endColorstr='#0094c4',GradientType=0 ); /* IE6-8 */
	}

/*Blocks within web site*/
