<div id="container">	
	<div id="content">
		<h1>New Grade</h1>
		<div id="MainText" class="gradient2">
			<% if notice %>
    			<p class="notice"><%= notice %></p>
			<% end %>
			<% if alert %>
				<p class="alert"><%= alert %></p>
			<% end %>
			<div class="field">
				<%= render 'form' %>
			</div>
			<div class="clearLeft">
				<%= link_to 'My Inventory', products_path %> |
				<%= link_to 'My Grades', grades_path %>
			</div>
		</div>
		<div class= "footer">
			<%=render :partial => 'layouts/footer'%>
		</div>
	</div>
</div>